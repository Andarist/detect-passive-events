{
  "name": "detect-passive-events",
  "version": "1.0.4",
  "description": "Detect if the browser supports passive events",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf es lib",
    "prebuild": "npm run clean",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src -d lib",
    "build:es": "cross-env BABEL_ENV=es babel src -d es",
    "prepublish": "npm run build"
  },
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafrex/detect-passive-events.git"
  },
  "keywords": [
    "detect",
    "passive",
    "passive events"
  ],
  "author": "Rafael Pedicini <code@rafrex.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rafrex/detect-passive-events/issues"
  },
  "homepage": "https://github.com/rafrex/detect-passive-events#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "^2.2.0",
    "rimraf": "^2.6.2"
  }
}
